{
  "compilerOptions": {
    "jsx": "react",
    "target": "es6",
    "module": "commonjs",
    "baseUrl": ".",
    "paths": {
      "@src/*": ["./src/*"],
      "@screens/*": ["./src/screens/*"],
      "@components/*": ["./src/components/*"],
      "@utils/*": ["./src/utils/*"],
      "@utils": ["./src/utils"],
      "@assets/*": ["./src/assets/*"],
      "@apis/*": ["./src/apis/*"],
      "@redux/*": ["./src/redux/*"],
      "@libs/*": ["./src/libs/*"]
    }
  },
  "exclude": ["node_modules"]
}
